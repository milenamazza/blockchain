res in wallets_features_classes_combined.iterrows():\n","    G_wallet.add_node(addr_id, **features.to_dict(), node_type=\"wallet\")\n","\n","# Funzione per aggiungere archi in batch con `chunksize`\n","def add_edges_from_file(graph, filename, col1, col2, chunksize=100000, edge_type=\"wallet-wallet\"):\n","    \"\"\"\n","    Aggiunge gli archi al grafo specificando il tipo di connessione\n","    \"\"\"\n","    for chunk in pd.read_csv(filename, usecols=[col1, col2], chunksize=chunksize):\n","        # Aggiungiamo gli archi e inferiamo il tipo da quale file provengono\n","        for u, v in chunk.itertuples(index=False, name=None):\n","            graph.add_edge(u, v, edge_type=edge_type)\n","\n","# Aggiungere gli archi con tipo specificato (wallet-wallet, wallet-tx)\n","add_edges_from_file(G_wallet, path + \"AddrAddr_edgelist.csv\", \"input_address\", \"output_address\", edge_type=\"wallet-wallet\")\n","add_edges_from_file(G_wallet, path + \"AddrTx_edgelist.csv\", \"input_address\", \"txId\", edge_type=\"wallet-tx\")\n","add_edges_from_file(G_wallet, path + \"TxAddr_edgelist.csv\", \"txId\", \"output_address\", edge_type=\"tx-wallet\")\n","\n","# Funzione per analizzare gli archi in base al tipo di connessione\n","def analyze_edges(graph):\n","    wallet_wallet_edges = [(u, v) for u, v, data in graph.edges(data=True) if data.get('edge_type') == 'wallet-wallet']\n","    wallet_tx_edges = [(u, v) for u, v, data in graph.edges(data=True) if data.get('edge_type') == 'wallet-tx' or data.get('edge_type') == 'tx-wallet']\n","\n","    # Statistiche sul numero di archi per tipo\n","    print(f\"Numero di archi wallet-wallet: {len(wallet_wallet_edges)}\")\n","    print(f\"Numero di archi wallet-transazione: {len(wallet_tx_edges)}\")\n","\n","# Analizzare gli archi\n","analyze_edges(G_wallet)\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"BSxm-KBfcHUF","executionInfo":{"status":"ok","timestamp":1742197312400,"user_tz":-60,"elapsed":181499,"user":{"display